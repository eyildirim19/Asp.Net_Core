@model List<Kullanici>

@{
	Layout = null;
}
<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Index</title>
</head>
<body>
		<table>
			<tr>
				<td>Kullanıcı Adı</td>
				<td>Şifre</td>
			</tr>
			@foreach (var item in Model)
			{
				<tr>
					<td>@item.KullaniciAdi</td>
					<td>@item.Sifre</td>
				</tr>
			}
		</table>
</body>
</html>